        <record id="view_employee_form456" model="ir.ui.view">
            <field name="name">view.employee.form.inherit</field>
            <field name="model">hr.employee</field>
            <field name="type">form</field>
	    <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
           
                   
           <xpath expr="/form/sheet/div[@class='oe_title']" position="attributes">
                    <attribute name="invisible">True</attribute>
           </xpath>
           
           <xpath expr="/form/sheet/notebook" position="before">
           <div class="oe_title">
           				
                            <label for="name" class="oe_edit_only"/>
                            
                            <h2>
                                <field name="name"/>
                            </h2>
                            
                            <label for="emp_first_name"/>
                           <h2>
                                
                                <field name="emp_first_name" placeholder="First Name"/>
                            </h2>
                            <label for="emp_middle_name"/>
                            
                             <h2>
                                
                                <field name="emp_middle_name" placeholder="Middle Name"/>
                            </h2>
                            <label for="emp_last_name"/>
                            
                             <h2>
                                
                                <field name="emp_last_name" placeholder="Last Name"/>
                            </h2>
                            <label for="identification_id1"/>
                            <h2>
                                
                                <field name="identification_id1"/>
                            </h2>
                            <!--label for="category_ids" class="oe_edit_only" groups="base.group_hr_user"/>
                            <field name="category_ids" widget="many2many_tags" placeholder="e.g. Part Time" groups="base.group_hr_user"/-->
                            <label for="category" class="oe_edit_only"/>
                            <field name="category" widget="selection"/>
                        </div>
           </xpath>
                      
            <xpath expr="/form/sheet/notebook/page[@string='Public Information']" position="attributes">
                    <attribute name="invisible">True</attribute>
           </xpath>
           
           <xpath expr="/form/sheet/notebook/page[@string='Public Information']" position="replace"> 
            <page string="Work Information">
            <group>
                  <group string="Contact Information">
                      <field name="address_id" on_change="onchange_address_id(address_id)" context="{'show_address': 1}" options='{"always_reload": True, "highlight_first_line": True}'/>
                      <field name="work_email" widget="email"/>
                      <field name="work_phone"/>
                      <field name="mobile_phone"/>
                      <field name="work_location"/>
                  </group>
                  <group string="Position">
                       <field name="department_id" on_change="onchange_department_id(department_id)"/>
                       <!--field name="job_id" options='{"no_open": True}' domain="[('state','!=','old')]" context="{'form_view_ref': 'hr.view_hr_job_employee_form'}" position="replace"/-->
                       <field name="parent_id"/>
                       <field name="coach_id"/>
                  </group>
                 <group>
                       <field name="company_id" groups="base.group_multi_company" on_change="onchange_company(company_id)"/>
                       <field name="user_id" on_change="onchange_user(user_id)" string="Related User"/>
                 </group>
              </group>
               <field name="notes" placeholder="Other Information ..." colspan="4"/>
            
            </page>
            </xpath>
            
              <xpath expr="/form/sheet/notebook/page[@string='HR Settings']" position="inside">
              <group>
              	<field name='lifeguard'/>
              	 <field name="attendance" />
              </group>
                    
           </xpath>
                
           
                  
           <xpath expr="/form/sheet/notebook/page[@string='Personal Information']" position="attributes">
                    <attribute name="invisible">True</attribute>
           </xpath>
                
            <xpath expr="/form/sheet/notebook" position="inside"> 
            <page string="Personal Information" groups="base.group_hr_user">
                 <group>
                 <group string="Personal Particulars">
                 <field name="blood_group"/>
           		 <field name="identification_marks"/>
           		
           		 </group>
           		
           		 </group>
           		 <group>
           		 <group string="Status">
                 <field name="gender"/>
                 <field name="marital"/>
                 <field name="marrage_date" attrs="{'invisible':[('marital', '=', 'Unmarried')]}"/>
                 <field name="no_of_children" attrs="{'invisible':[('marital', '=', 'Unmarried')]}"/>
                 </group>
                 <group string="Birth Details">
                 	<field name="birthday"/>
                 	<field name="place_of_birth"/>
                 	<!--field name="birth_city"/-->
                 
                </group>
           		</group>
           		 
           		 <group>
           		 <group string="Contact Details">
           		 <field name="emergency_contact"/>
           		 <field name="telephone_no1"/>
           		 <field name="mobile_no1"/>
           		 <field name="email_id"/>
           		 </group>
          		 <group string="Physical Particulars">
					
					<field name="height"/>
					<field name="weight"/>
					<field name="complexion"/>
				</group>
				</group>
			
			 <!--label for="street1" string=" Current Address"/-->
			 <group col ="2" colspan="2" string="Current Address" >
			 
		          <field name="street1" placeholder="Street1..."  attrs="{'invisible': [('goa_employee','=',True)]}" nolabel="1"/>
                 	<field name="street2" placeholder="Street2..."  attrs="{'invisible': [('goa_employee','=',True)]}" nolabel="1"/>
                 
                 <div class="address_format">
                     <field name="current_city" placeholder="City" style="width: 40%%" attrs="{'invisible': [('goa_employee','=',True)]}" nolabel="1"/>
                     <field name="Current_state_id" class="oe_no_button" placeholder="State" style="width: 37%%" options='{"no_open": True}'  attrs="{'invisible': [('goa_employee','=',True)]}" nolabel="1"/>
                     <field name="pincode" placeholder="ZIP" style="width: 20%%" attrs="{'invisible': [('goa_employee','=',True)]}" nolabel="1"/>
                      
                 </div>
           		 <field name ="landmark" placeholder="Landmark" style="width: 20%%" attrs="{'invisible': [('goa_employee','=',True)]}" nolabel="1"/>
           		 <field name="country_id" placeholder="Country" class="oe_no_button" options='{"no_open": True}' attrs="{'invisible': [('goa_employee','=',True)]}" nolabel="1"/>
           	</group>
           		
			
           		 
           	<group col="2" colspan="2" string="Permanent Address">
           		<field name="goa_employee"/>
           		 <field name="same_current_address"/>
           		 <field name="house_no" attrs="{'invisible': [('goa_employee','!=',True)]}"/>
           		 <field name="ward" attrs="{'invisible': [('goa_employee','!=',True)]}"/>
           		 <field name="village"/>
           		 <field name="municipality" attrs="{'invisible': [('goa_employee','!=',True)]}"/>
           		 <field name="district" attrs="{'invisible': [('goa_employee','!=',True)]}"/>
           		 <field name="constituency" attrs="{'invisible': [('goa_employee','!=',True)]}"/>
           		 <field name="taluka" attrs="{'invisible': [('goa_employee','!=',True)]}"/>
           		 <field name="state_name"/>
           		 <field name="post_office" attrs="{'invisible': [('goa_employee','!=',True)]}"/>
           		 <field name="pincode"/>
           		 <field name="landmark"/>
           		 
           		 <group col="2" colspan ="2" attrs="{'invisible':[('goa_employee','!=',False)]}">
           		 <field name ="street1"/>
           		 <field name ="street2"/>
           		 <field name="country_id"/>
           		 </group>
           	
           		 <field name="other"/>
				</group>
			
				<group string="Citizenship &amp; Other Info">
                <field name="country_id"/>
                
                <field name="passport_id" groups="base.group_hr_user"/>
                
                <field name="otherid" groups="base.group_hr_user"/>
                </group>
				 <group string="Identification Proof Details">
           		 
           		 <field name="employee_identification_proof_detail" colspan="4" nolabel="1"/>
           		 </group>
           		 
           		<group col="2" colspan="2" string="Language Proficiency">
					
					<field name="language_line" widget="one2many" colspan="4"/>
				</group> 
           		 
				<group col="2" colspan="2" string="Family Particulars">
					
					<field name="father_name"/>
					<field name="family_line" widget="one2many" string="Family Particulars" nolabel="1" colspan="4"/>
				</group>
				<group col="2" colspan="2" string="Nominee Details">
           		 <field name="nominee"/>
           		 <field name="relationship"/>
           		 <field name="address"/>
           		 <field name="telephone_no"/>
           		 <field name="mobile_no"/>
           		 </group>
				
				
				</page>
			       </xpath>
				
				<xpath expr="/form/sheet/notebook" position="inside">
				<page string="Job Details">
				<group>
				<group>
				<!--field name="identification_id" string="SLSG No."/-->
				<field name="job_id" string="Job Title"/>
				</group>
				<group>
				<field name="app_letter"/>
           		<field name="doj"/>
           		<field name="emoluments"/>
           		</group>
           		</group>
           		<group>
           		<group> 
           		<field name="esic_no"/>
           		<field name="pf_no"/>
           		</group>
           		<group>
           		<field name="employment_exchange_no"/>
           		<field name="distance"/>
           		</group>
           		</group>
				
				</page>
				
            	<page string="Qualification Details">
     			<group col="2" colspan="2" string="Professional Qualifications">
					
					<field name="professional_qualification_line" widget="one2many" string="Professional Qualifications" nolabel="1" colspan="4"/>
				</group>
            	
           		 <group col="2" colspan="2" string="Educational Qualifications">
					
					<field name="educational_qualification_line" widget="one2many" string="Educational Qualifications" nolabel="1" colspan="4"/>
				</group>
				<group col="2" colspan="2" string="Other Qualifications">
					
					<field name="other_qualification_line" widget="one2many" string="Other Qualifications" nolabel="1" colspan="4"/>
				</group>
            	</page>
            	
            	<page string="Bank Particulars">
   	           		
   	           		 <group col="4">
                        
                        <field name="acc_number" placeholder="Account Number"/>
                       <!-- <field name="company_id" groups="base.group_multi_company" on_change="onchange_company_id(company_id)"
                            invisible="context.get('company_hide', True)" widget="selection"/> -->
                        <field name="footer" invisible="context.get('footer_hide', True)"/>
                    </group>
                    <group>
                        
                        <group name="bank" string="Information About the Bank">
                            <!--field name="bank" on_change="onchange_bank_id(bank)"/-->
			                <field name="bank_field"/>
			                <field name="branch_name"/>
                            <!--field name="bank_name" attrs="{'required': [('company_id','&lt;&gt;',False)]}"/-->
                            <field name="bank_bic" placeholder="e.g. GEBABEBB" />
                        </group>
                            
                        </group>              
   	           		 
            	</page>
            	
            	<page string="Lifeguards Training Section" attrs="{'invisible':[('lifeguard', '=', False)]}">
                
       		    <group col="2" colspan="2" string="Beach Lifeguard Details">
					
					<field name="beach_line" widget="one2many" string="Beach Lifeguard Details" nolabel="1" colspan="4"/>
				</group>
				<group col="2" colspan="2" string="Jetski (Surf Rescue) Operator">
					
					<field name="jetski_line" widget="one2many" string="Jetski (Surf Rescue) Operator" nolabel="1" colspan="4"/>
				</group>
            	
            	
            	</page>
            	
            	<page string="Performance History">
            	<group col="2" colspan="2" string="Annual Assessment">
            	<field name="annual_line" widget="one2many" string="Annual Assessment" nolabel="1" colspan="4"/>
            	</group>
            	<group col="2" colspan="2" string="Achievements/Certificates">
            	<field name="achievement_line" widget="one2many" string="Achievements/Certificates" nolabel="1" colspan="4"/>
            	</group>
            	<group col="2" colspan="2" string="Offence Details">
            	<field name="offence_line" widget="one2many" string="Offence Details" nolabel="1" colspan="4"/>
            	</group>
            	</page>
                <page string="Attachments">
                <group>
                <field name="attachment_line" widget="one2many" string="Attachments" nolabel="1" colspan="4"/>
                </group>
                </page>           	
            	
				<page string="Previous Job History">
				<group col="2" colspan="2" string="Previous Job History">
				<field name="previous_job_history" nolabel="1"/>
				</group>
				
				</page>
                </xpath>
           		
           
            </field>
          
        </record>
        
